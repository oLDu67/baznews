{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "zizaco/entrust": "5.2.x-dev",
        "laravelcollective/html": "5.3.*",
        "caffeinated/themes": "^3.0",
        "rinvex/repository": "2.0.*",
        "caffeinated/modules": "^4.0",
        "kalnoy/nestedset": "^4.1",
        "venturecraft/revisionable": "1.*",
        "intervention/image": "^2.3",
        "unisharp/laravel-filemanager": "^1.6",
        "unisharp/laravel-ckeditor": "^4.5",
        "mews/purifier": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.1",
        "spatie/laravel-backup": "^4.0",
        "dingo/api": "1.0.x@dev",
        "predis/predis": "^1.1",
        "roumen/feed": "^2.10",
        "rap2hpoutre/laravel-log-viewer": "^0.7.0",
        "arrilot/laravel-widgets": "^3.6",
        "laravel/socialite": "^2.0",
        "laravel/scout": "^1.1",
        "algolia/algoliasearch-client-php": "^1.11",
        "geerlingguy/ping": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2"

    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"

        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts":{
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}

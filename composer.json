{
    "name": "oLDu67/baznews",
    "description": "Baznews is online newspaper cms.",
    "keywords": ["online newspaper", "baznews", "laravel", "cms"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "zizaco/entrust": "5.2.x-dev",
        "rinvex/repository": "dev-develop",
        "caffeinated/modules": "^4.0",
        "caffeinated/themes": "^4.0",
        "kalnoy/nestedset": "^4.3",
        "venturecraft/revisionable": "^1.28",
        "intervention/image": "^2.4",
        "unisharp/laravel-filemanager": "^1.8",
        "unisharp/laravel-ckeditor": "^4.6",
        "mews/purifier": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.3",
        "spatie/laravel-backup": "^5.0",
        "predis/predis": "^1.1",
        "roumen/feed": "^2.11",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "arrilot/laravel-widgets": "^3.7",
        "algolia/algoliasearch-client-php": "^1.20",
        "geerlingguy/ping": "^1.1",
        "brotzka/laravel-dotenv-editor": "2.*",
        "laravelcollective/html": "^5.5",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "mremi/url-shortener": "dev-master",
        "patchwork/utf8": "^1.3",
        "laravel/passport": "^4.0",
        "spatie/laravel-fractal": "^5.2",
        "illuminate/support": "^5.5",
        "illuminate/events": "^5.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"

        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}

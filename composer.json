{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.5.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "zizaco/entrust": "5.2.x-dev",
        "rinvex/repository": "dev-develop",
        "caffeinated/modules": "^4.0",
        "caffeinated/themes": "^4.0",
        "kalnoy/nestedset": "^4.3",
        "venturecraft/revisionable": "^1.28",
        "intervention/image": "^2.4",
        "unisharp/laravel-filemanager": "^1.8",
        "unisharp/laravel-ckeditor": "^4.6",
        "mews/purifier": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.3",
        "spatie/laravel-backup": "^5.0",
        "predis/predis": "^1.1",
        "roumen/feed": "^2.11",
        "rap2hpoutre/laravel-log-viewer": "^0.7.0",
        "arrilot/laravel-widgets": "^3.6",
        "algolia/algoliasearch-client-php": "^1.11",
        "geerlingguy/ping": "^1.1",
        "brotzka/laravel-dotenv-editor": "2.*",
        "laravelcollective/html": "^5.5",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "mremi/url-shortener": "dev-master",
        "patchwork/utf8": "^1.3",
        "laravel/passport": "^3.0",
        "spatie/laravel-fractal": "^5.2",
        "illuminate/support": "^5.5",
        "illuminate/events": "^5.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0"

    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"

        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts":{
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
